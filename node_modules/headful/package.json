{
  "name": "headful",
  "version": "1.0.0",
  "description": "Set <head> properties (e.g. title and meta tags) from JavaScript.",
  "keywords": [
    "head",
    "title",
    "meta",
    "metatag",
    "metatags",
    "seo",
    "sharing"
  ],
  "homepage": "https://github.com/troxler/headful",
  "bugs": "https://github.com/troxler/headful/issues",
  "license": "MIT",
  "author": "troxler",
  "files": [
    "dist",
    "readme.md"
  ],
  "main": "dist/headful.js",
  "repository": "github:troxler/headful",
  "scripts": {
    "dev": "(cd demo; poi)",
    "test": "ava",
    "build": "babel src/ --presets babel-preset-env --out-dir dist/",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "0.24.0",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "jsdom": "11.5.1",
    "poi": "9.6.10"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "dist/**/*"
    ],
    "failFast": true,
    "failWithoutAssertions": true
  }
}
